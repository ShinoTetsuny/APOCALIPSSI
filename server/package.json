{
  "name": "pdf-analyzer-server",
  "version": "1.0.0",
  "description": "Backend pour l'analyse de documents PDF",
  "main": "server.js",
  "scripts": {
  "start": "node server.js",
  "dev": "nodemon server.js",
  "start:backend": " npm run dev",
  "start:frontend": "cd.. && cd client && npm start",
  "start:ollama": "start cmd /k \"ollama run mistral\"",
  "start:all": "concurrently \"npm run start:ollama\" \"npm run start:backend\" \"npm run start:frontend\"",
  "test": "jest --runInBand"
}
,
  "keywords": [
    "pdf",
    "analysis",
    "openai",
    "nodejs",
    "express"
  ],
  "author": "PDF Analyzer Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "jest": "^30.0.3",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
